    OE2RP_DWARKAR_182 = "Criminal!";
    OE2RP_DWARKAR_183 = "You've been caught and put in jail! Your supplies are in a storage chest. The only items in your possession are the wand, the familiar, and a wellhidden lock pick.";
    OE2RP_DWARKAR_184 = "[BAM! SLAM!!]";
