    OE2RP_ELLANDRA_90 = "Imma... uuhh… *hic* yep! Have keys to both cells! *hic*";
    OE2RP_ELLANDRA_91 = "Could you open my cell?";
    OE2RP_ELLANDRA_92 = "Um… sure.";
    OE2RP_ELLANDRA_93 = "*yawn* Tired *hic*";
    OE2RP_ELLANDRA_94 = "Well, get yourself some rest. Umm, I'll check on you later!";
