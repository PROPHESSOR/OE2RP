    OE2RP_B_ID595 = "@HOW TO MOVE";
    OE2RP_B_ID596 = "2 move forward";
    OE2RP_B_ID597 = "8 move back";
    OE2RP_B_ID598 = "1 move left";
    OE2RP_B_ID599 = "3 move right";
    OE2RP_B_ID600 = "4 turn left/page up";
    OE2RP_B_ID601 = "6 turn right/page dwn";
    OE2RP_B_ID602 = "If supported by your handset, you can also use the arrow keys on your directional pad to move and turn.";
    OE2RP_B_ID603 = "UP move forward";
