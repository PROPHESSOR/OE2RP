    OE2RP_B_ID629 = "$ Shrunk";
    OE2RP_B_ID630 = "$ Strength";
    OE2RP_B_ID631 = "@ITEM HELP";
    OE2RP_B_ID632 = "You will encounter potions and other items to aid your progress in the game. These items may be viewed and used through the Inventory menu.";
    OE2RP_B_ID633 = "Using an item takes one turn. After using an item, if you are attacked, you'll return to the game; otherwise you can stay in the menu and continue to use items.";
    OE2RP_B_ID634 = "@EFFECTS";
    OE2RP_B_ID635 = "Item effects can last a number of turns. Active effects are shown at the righthand side of the display, along with its remaining turns.";
    OE2RP_B_ID636 = "@TIPS";
    OE2RP_B_ID637 = "Remedy potion clears you of all negative effects.";
    OE2RP_B_ID638 = "Don't waste money on trinkets.";
    OE2RP_B_ID639 = "@LOCK PICKING";
    OE2RP_B_ID640 = "To initiate lock pick mode, face lock and press OK (key 5). Press UP (key 2), DOWN (key 8), LEFT (key 4), or RIGHT (key 6) to start setting the current tumbler. Press the same key again to finish setting the tumbler. Continue till all tumblers are set.";
    OE2RP_B_ID641 = "";
