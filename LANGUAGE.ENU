    OE2RP_ELLANDRA_150 = "Frolim";
    OE2RP_ELLANDRA_151 = "Uhh hello! *hic* Would you be so kind *hic* as to purchase some wine for me?";
    OE2RP_ELLANDRA_152 = "I'm a bit short on gold. You should purchase it yourself.";
    OE2RP_ELLANDRA_153 = "*grumble* The barkeep refuses to *hic* sell me anymore! *hic*";
