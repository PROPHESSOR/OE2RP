    OE2RP_B_ID611 = "9 pass turn/skip";
    OE2RP_B_ID612 = "@ARMOR HELP";
    OE2RP_B_ID613 = "Armor protects against damage from enemies. Quality of armor has two traits: Deflection and Durability.";
    OE2RP_B_ID614 = "@DEFLECTION";
    OE2RP_B_ID615 = "Deflection is the max amount of damage your armor protects in a single attack.";
    OE2RP_B_ID616 = "@DURABILITY";
    OE2RP_B_ID617 = "Durability is the total number of attacks your armor has before it drops to zero. On the HUD, armor's durability is shown next to shield icon.";
    OE2RP_B_ID618 = "If armor's durability is low, use Armor Kit to repair it, or purchase new armor.";
    OE2RP_B_ID619 = "@EFFECTS LIST";
    OE2RP_B_ID620 = "$ Accuracy";
    OE2RP_B_ID621 = "$ Avoidance";
    OE2RP_B_ID622 = "$ Defense";
