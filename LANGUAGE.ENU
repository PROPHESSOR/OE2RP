    OE2RP_B_ID586 = "@HOW TO PLAY";
    OE2RP_B_ID587 = "Use the dpad or number keys to move around the environment and navigate menus. Press OK (key 5) to talk, attack, and perform other actions.";
    OE2RP_B_ID588 = "@OTHER";
    OE2RP_B_ID589 = "5 attack/talk/use";
    OE2RP_B_ID590 = "9 pass turn/skip";
    OE2RP_B_ID591 = "# view map";
