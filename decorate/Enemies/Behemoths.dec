Actor Behemoth: OE2BaseMonster {
    Tag "$OE2RP_M_BEHEMOTH"

    States {
        Spawn:
            BEH1 A -1
            Stop
    }
}

Actor RedBehemoth: Behemoth {
    Tag "$OE2RP_M_BEHEMOTH"

    States {
        Spawn:
            BEH2 A -1
            Stop
    }
}

Actor ShadowBehemoth: Behemoth {
    +SHADOW

    Tag "$OE2RP_M_SHADOW_BEHEMOTH"
    RenderStyle "Shaded"
    StencilColor "white"
}

