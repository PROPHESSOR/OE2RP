Actor OE2Familiar: BaseDecoration {
}

Actor OE2Fog: BaseDecoration {
	Alpha 0.75
	VSpeed 0.3
	RenderStyle "Translucent"
	+NOGRAVITY

	States {
	Spawn:
		AFOG A 3 A_SetTranslucent( clamp( alpha + FRandom( -0.05, 0.05 ), 0.55, 0.8 ) )
		Loop
	}
}

Actor OE2VioletFog: OE2Fog {
	States {
	Spawn:
		AFOG B 3 A_SetTranslucent( clamp( alpha + FRandom( -0.05, 0.05 ), 0.55, 0.8 ) )
		Loop
	}
}
